{
  "name": "@cohook/preact",
  "version": "1.1.0-beta.0",
  "description": "preact lightweight, progressive data caching and cross-component communication",
  "author": "Keylenn <darklight_on@163.com>",
  "homepage": "https://github.com/Keylenn/cohookjs/tree/master/packages/cohook-preact#readme",
  "license": "MIT",
  "main": "dist/index.js",
  "umd:main": "dist/preact.umd.production.min.js",
  "unpkg": "dist/preact.umd.production.min.js",
  "jsdelivr": "dist/preact.umd.production.min.js",
  "jsnext:main": "dist/preact.esm.production.min.js",
  "module": "dist/preact.esm.js",
  "react-native": "dist/preact.esm.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "src",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Keylenn/cohookjs.git"
  },
  "keywords": [
    "preact",
    "hooks",
    "cohook",
    "immutable",
    "container",
    "effect"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "test": "tsdx test",
    "build": "tsdx build --format cjs,esm,umd",
    "build:check": "yarn lint && yarn test && yarn build && yarn size",
    "lint": "eslint src --ext js,ts",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "release:alpha": "yarn build:check && standard-version --prerelease alpha && npm publish",
    "release:beta": "yarn build:check && standard-version --prerelease beta && npm publish",
    "release": "yarn build:check && standard-version && npm publish"
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/Keylenn/cohookjs/issues"
  },
  "size-limit": [
    {
      "path": "dist/preact.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/preact.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^5.0.4",
    "@testing-library/preact": "^2.0.1",
    "preact": "^10.5.14",
    "size-limit": "^5.0.4",
    "standard-version": "^9.3.1",
    "tsdx": "^0.14.1"
  },
  "dependencies": {
    "@cohook/core": "1.0.0-beta.1"
  },
  "peerDependencies": {
    "preact": "^10"
  }
}
